{
    "name": "@arkitektum/altinn-studio-custom-components",
    "version": "13.0.0",
    "description": "Custom components for Altinn Studio",
    "main": "dist/main.js",
    "module": "dist/main.js",
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "test": "jest",
        "copy-dist-resources": "copyfiles --flat src/data/resource.nb.json dist",
        "build": "webpack --mode production && yarn copy-dist-resources",
        "copy-docs-resources": "copyfiles --flat src/data/resource.nb.json docs/data",
        "build-docs": "webpack --mode production --config webpack.config.docs.js && yarn copy-docs-resources",
        "start": "webpack serve --mode development",
        "start-docs": "webpack serve --mode development --config webpack.config.docs.js",
        "lint": "eslint src --ext .js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Arkitektum/altinn-studio-custom-components.git"
    },
    "author": "",
    "bugs": {
        "url": "https://github.com/Arkitektum/altinn-studio-custom-components/issues"
    },
    "homepage": "https://github.com/Arkitektum/altinn-studio-custom-components#readme",
    "devDependencies": {
        "@babel/preset-env": "^7.26.9",
        "@eslint/js": "^9.34.0",
        "copyfiles": "^2.4.1",
        "css-loader": "^7.1.2",
        "css-minimizer-webpack-plugin": "^7.0.0",
        "date-fns": "^4.1.0",
        "date-fns-tz": "^3.2.0",
        "eslint": "^9.34.0",
        "globals": "^16.3.0",
        "highlight.js": "^11.11.1",
        "html-webpack-plugin": "^5.6.4",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jsdom": "^26.0.0",
        "mini-css-extract-plugin": "^2.9.2",
        "prettier": "3.4.2",
        "style-loader": "^4.0.0",
        "webpack": "^5.97.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.0"
    },
    "packageManager": "yarn@4.10.3"
}
